button {
   background-color: transparent;
   padding: 0;
   margin: 0;
   position: static;
   border: 0;
   border-radius: 0;
   font-size: 0rpx;
   color: transparent;
}
button::after {
  content: '';
  width: 0;
  height: 0;
  -webkit-transform:scale(1);
  transform:scale(1);
  display: none;
  background: transparent;
}

image {
  width: 100%;
}

view {
  box-sizing: border-box;
}
